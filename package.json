{
  "name": "perceptron",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "start": "npm run preview",
    "dump-roles": "npm exec supabase db dump -- -f supabase/roles.sql --role-only",
