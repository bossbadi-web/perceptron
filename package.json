    "svelte": "^4.0.5",
    "sveltekit-flash-message": "^2.4.2",
    "vite": "^4.5.2"
  },
  "type": "module",
  "dependencies": {
    "@supabase/auth-helpers-sveltekit": "^0.10.3",
    "@supabase/supabase-js": "^2.36.0",
    "leo-profanity": "^1.7.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.8",
    "ocr-space-api-wrapper": "^2.3.0",
    "openai": "^4.19.1",
